digraph G {
min[label="min" pos="0,0!"];
subgraph {
n2[label="2" pos="0,-1!"];
}
subgraph {
n3[label="3" pos="1,-1!"];
}
subgraph {
n4[label="4" pos="2,-1!"];
}
min -> n2[label=""];
n2 -> n3[label="n"];
n2:s -> n4:s[label="p"];
n3 -> n4[label="n"];
n3 -> n2[label="p"];
n4:n -> n2:n[label="n"];
n4 -> n3[label="p"];
}