digraph G {
min[label="min" pos="0,0!"];
subgraph {
n3[label="3" pos="0,-1!"];
subgraph {
n4[label="4" pos="0,-2!"];
}
n3 -> n4[label="d"];
n4 -> n4[label="n"];
n4 -> n4[label="p"];
n4 -> n3[label="u"];
}
subgraph {
n15[label="15" pos="2,-1!"];
}
min -> n3[label=""];
n3 -> n15[label="n"];
n3:s -> n15:s[label="p"];
n15:n -> n3:n[label="n"];
n15 -> n3[label="p"];
}